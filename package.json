{
  "name": "e-commerce-369",
  "version": "1.0.0",
  "description": "Saudi market-focused women's clothing e-commerce platform built with Next.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "deploy:staging": "./deploy-cloudflare.sh staging",
    "deploy:production": "./deploy-cloudflare.sh production",
    "cf:login": "wrangler login",
    "cf:deploy": "wrangler pages deploy dist --project-name=e-commerce-369",
    "cf:tail": "wrangler tail",
    "cf:kv:list": "wrangler kv:namespace list",
    "cf:d1:list": "wrangler d1 list",
    "cf:r2:list": "wrangler r2 bucket list"
  },
  "keywords": [
    "e-commerce",
    "saudi",
    "women-clothing",
    "arabic",
    "payment-integration",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "author": "Mohamed El Fadil Abuagla",
  "license": "MIT",
  "dependencies": {
    "next": "^15.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "wrangler": "^3.89.0"
  }
}
